{
    "version": 2,
    "name": "checkin-backend",
    "builds": [
       { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/index.js" }
    ],
    "crons": [
      {
        "path": "/api/admin/auto-check",
        "schedule": "0 */6 * * * " 
      }
    ]
  
 }
 