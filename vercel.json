{
    "version": 2,
    "name": "checkin-backend",
    "builds": [
       { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/index.js" }
    ],
    "crons": [
      {
        "path": "/api/admin/auto-check",
        "schedule": "0 19 * * *" 
      },{
         "path": "/api/admin/auto-check",
         "schedule": "0 1 * * *" 
      },{
         "path": "/api/admin/auto-check", 
         "schedule": "0 7 * * *" 
       },{
         "path": "/api/admin/auto-check",
         "schedule": "0 13 * * *" 
       }
    ]
  
 }
 